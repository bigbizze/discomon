{
  "name": "discomon",
  "version": "1.0.0",
  "description": "mon game for discord",
  "main": "./prod/bot.js",
  "types": "./prod/bot.d.ts",
  "scripts": {
    "build": "yarn format && tsc",
    "format": "eslint --fix",
    "lint": "eslint",
    "prepare" : "yarn build",
    "clean": "rimraf node_modules && yarn install",
    "build-private-server": "python3 build_project.py -s private_test",
    "build-test-server": "python3 build_project.py -s public_test",
    "build-live-server": "python3 build_project.py -s live",
    "run": "node ./src/discomon.ts --require ts-node/register --require @babel/register",
    "test": "jest --watch",
    "kill": "taskkill /f /im node.exe",
    "re-db": "node --require ./babel-register.cjs --require ts-node/register --require @babel/register ./src/setup_scripts/flush_db.ts && node --require ./babel-register.cjs --require ts-node/register --require @babel/register ./src/setup_scripts/build_db.ts"
  },
  "author": "bigbizze & p4stoboy",
  "license": "ISC",
  "dependencies": {
    "@2toad/profanity": "^1.1.0",
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/register": "^7.10.5",
    "@microsoft/signalr": "^3.1.8",
    "@types/color-convert": "^1.9.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.162",
    "@types/mysql": "^2.15.15",
    "@types/node": "^14.0.11",
    "@types/node-fetch": "^2.5.7",
    "@types/redux-devtools-extension": "^2.13.2",
    "@types/typescript": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "app-root-path": "^3.0.0",
    "babel-jest": "^26.0.1",
    "body-parser": "^1.19.0",
    "canvas": "^2.6.1",
    "color-convert": "^2.0.1",
    "compressing": "^1.5.1",
    "cors": "^2.8.5",
    "date-fns": "^2.14.0",
    "dblapi.js": "^2.4.1",
    "delta-e": "0.0.8",
    "discord.js": "12.3.1",
    "dotenv": "^8.2.0",
    "eslint": "^8.5.0",
    "express": "^4.17.1",
    "integer": "^3.0.1",
    "jest": "^26.0.1",
    "mariadb": "^2.4.2",
    "mersenne-twister": "^1.1.0",
    "mysql": "^2.18.1",
    "mysql-tz": "^0.0.1",
    "node-fetch": "^2.6.1",
    "pureimage": "^0.2.4",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "tslint-immutable": "^6.0.1",
    "typescript": "^3.9.7"
  }
}
